{
  "name": "challengames-ui-vuejs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "querystringify": ">=2.0.0",
    "vue": "^2.6.10",
    "vue-flag-icon": "^1.0.6",
    "vue-i18n": "^8.11.2",
    "vue-router": "^3.0.6",
    "vuetify": "^1.5.5",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-eslint-parser": "^7.0.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.0.5"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier/vue",
      "plugin:vue/recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "es6": true,
      "browser": true
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "warn",
      "arrow-parens": [
        "warn"
      ],
      "semi": [
        "warn"
      ],
      "quotes": [
        "error"
      ]
    }
  },
  "prettier": {
    "arrowParens": "always"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
